{
    "type": "module",
    "scripts": {
        "test": "jest"
    },
    "jest": {
        "transform": {
            "\\.[jt]sx?$": "babel-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?)$",
        "moduleFileExtensions": [
            "js",
            "jsx",
            "json",
            "node"
        ]
    },
    "devDependencies": {
        "@babel/plugin-transform-modules-commonjs": "^7.18.6",
        "jest": "^29.2.0"
    },
    "babel": {
        "env": {
            "test": {
                "plugins": ["@babel/plugin-transform-modules-commonjs"]
            }
        }
    }
}
